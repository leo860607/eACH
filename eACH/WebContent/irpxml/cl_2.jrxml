	<queryString>
		<![CDATA[SELECT a.BANKID || '-' || (select coalesce(bgbk_name,'') from bank_group where bgbk_id=a.BANKID) BANKID
            , a.PCODE || ( SELECT coalesce(EACH_TXN_NAME,'') From EACH_TXN_CODE Where EACH_TXN_ID=a.PCODE)  PCODE
            ,a.CLEARINGPHASE
            , a.RECVCNT
            , a.RECVAMT
            , a.PAYCNT
            , a.PAYAMT
            , a.RVSRECVCNT
            , a.RVSRECVAMT --扣款沖正金額
            , a.RVSPAYCNT
            , a.RVSPAYAMT --入帳冲正金額
            ,a.RECVAMT+a.RVSRECVAMT in_tol --應收小計(入帳金額+扣款沖正金額) A
            ,a.PAYAMT+a.RVSPAYAMT out_tol --應付小計(扣款金額+入帳沖正金額) B
            ,((a.RECVAMT+a.RVSRECVAMT)-(a.PAYAMT+a.RVSPAYAMT)) dif_tol --差額 A-B
FROM
	ONCLEARINGTAB a

ORDER BY BANKID]]>
	</queryString>
